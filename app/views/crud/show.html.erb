<%= render :partial => "show_header" %>

<% (resource_class.column_names - ["id", "created_at", "updated_at"] ).each do |column| %>
  <div class="group">
    <label><%= t("activerecord.attributes.#{resource.class.to_s.downcase}.#{column}") %>:</label> <%= resource.send(column) %>
  </div>
<% end %>

